{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "extends": [
    "react-app",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "import/no-named-as-default": 0,
    "react/jsx-props-no-spreading": 0
  }
}

