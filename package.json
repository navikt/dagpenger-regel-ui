    "nav-frontend-toggle-style": "^0.0.25",
    "nav-frontend-typografi": "^2.0.12",
    "nav-frontend-typografi-style": "^1.0.13",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^3.1.0",
    "react-collapse": "^4.0.3",
    "react-dom": "^16.8.6",
    "react-modal": "^3.8.2",
    "react-motion": "^0.5.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.5",
    "less": "^3.5.2",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.14.1",
    "lint-staged": "^9.2.5",
