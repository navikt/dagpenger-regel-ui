{
  "name": "dagpenger-regel-ui",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://app.adeo.no/inntekter",
  "scripts": {
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:css": "less-watch-compiler --config less.json",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "start": "npm-run-all -p start:*",
    "build": "npm run build:css && npm run build:js",
    "test": "react-scripts test --env=jsdom",
    "test:less": "stylelint \"src/**/*.less\"",
    "eject": "react-scripts eject",
    "up": "docker-compose -f docker-compose.yml up --build",
    "down": "docker-compose -f docker-compose.yml down",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@sentry/browser": "^5.16.1",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-alpha.36",
    "formik": "^2.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "nav-frontend-alertstriper": "^3.0.2",
    "nav-frontend-alertstriper-style": "^2.0.2",
    "nav-frontend-chevron-style": "^0.3.4",
    "nav-frontend-core": "^4.0.9",
    "nav-frontend-ekspanderbartpanel": "^1.0.28",
    "nav-frontend-ekspanderbartpanel-style": "^0.3.20",
    "nav-frontend-ikoner-assets": "^1.0.1",
    "nav-frontend-js-utils": "^1.0.7",
    "nav-frontend-knapper": "^1.0.29",
    "nav-frontend-knapper-style": "^0.3.25",
    "nav-frontend-lukknapp": "^1.0.21",
    "nav-frontend-lukknapp-style": "^0.2.19",
    "nav-frontend-modal": "^1.0.28",
    "nav-frontend-modal-style": "^0.3.29",
    "nav-frontend-paneler": "^1.0.17",
    "nav-frontend-paneler-style": "^0.3.15",
    "nav-frontend-skjema": "^1.0.67",
    "nav-frontend-skjema-style": "^1.0.40",
    "nav-frontend-spinner": "^1.0.18",
    "nav-frontend-spinner-style": "^0.2.5",
    "nav-frontend-toggle": "^0.0.23",
    "nav-frontend-toggle-style": "^0.0.22",
    "nav-frontend-typografi": "^2.0.12",
    "nav-frontend-typografi-style": "^1.0.13",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-collapse": "^4.0.3",
    "react-dom": "^16.8.6",
    "react-modal": "^3.8.2",
    "react-motion": "^0.5.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.4.1",
    "snyk": "^1.334.0"
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.0",
    "less": "^3.5.2",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.13.0",
    "lint-staged": "^9.2.0",
    "nock": "^10.0.6",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6",
    "stylelint": "^10.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.jsx": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "snyk": true
}
